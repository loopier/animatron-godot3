def /color/purewhite actor
     /color $actor 1 1 1

def /color/white actor
     /color $actor 0.97 0.95 0.87

def /color/black actor
     /color $actor 0 0 0

def /color/darkblue actor
     /color $actor 0.05 0.08 0.09

def /color/blue actor
     /color $actor 0.21 0.28 0.32

def /color/midblue actor
    /color $actor 0.18 0.32 0.4

def /color/lightblue actor
    /color $actor 0.45 0.58 0.66

def /color/orange actor
     /color $actor 0.91 0.63 0.29

def /color/red actor
     /color $actor 0.32 0.05 0.08

def /color/darkgray actor
     /color $actor 0.12 0.17 0.20

def /color/gray actor
     /color $actor 0.59 0.61 0.62

def /setup
    /free *
    /assets/path /home/r/work/toplap-20221005-asturies/anims
    /list/assets
    /load circle-hd
    /load h-static
    /load circle-dash
    /load circle
    /load circle-ln
    /load circle-dash-long
    /load ear
    /load garbuix-1
    /load leaf
    /load line-in
    /load line-long
    /load dash
    /load dash-in
    /load dash-inout
    /load dot-xl
    /load dot-l
    /load dot-s
    /load dot-xs
    /load dot-grow
    /load drop
    /load line-2
    /load rect-long-l
    /load rect-long
    /load rect-long-s
    /load square
    /load swirl-1
    /load twirl-1
    /load twirl-2
    /load cpu-keyboard
    /load flicker
    /load robot-hr
    /list/actors
    /black

def /reverse actor
    /play $actor 1

def /full name
    /new $name square
    /scale $name "2,1.1"

def /dots num
    /create/group dots dot-grow $num
    /color/white dots

def /twirls num
    /create/group tw twirl-1 $num
    /color/white tw
    /fliph tw
    /scale  tw 0.25
    /speed  tw 0.65
    /position  tw 0.63, 0.26
    /pivot  tw 1.0, 0.5

def /squareleft
    /new  square square
    /behind  square cpu
    /position  square -0.5, 0.5
    /position  square 0.2, 0.5, 0.5
    /speed  square 0.6
    /color/darkgray square

def /swirls num
    /create/group sw swirl-1 $num
    /position  sw 0.25, 0.5
    /color/white sw
    /play sw 1

def /squares num
    /create/group sq square $num
    /position sq 0.75 0.5
    /color/darkgray sq
    /speed sq 0.5

def /lines num
    /create/group lines line-long $num
    /color/orange lines

def /circles num
    /create/group circles circle-hd $num

