def /black
    /new black square
    /color/black black
    /scale black "2,1.1"
    /fade black 1

def /fadeout time
    /fade black 0
    /front black *
    /fade black 1 $time

def /fadein time
    /fade black 1
    /front black *
    /fade black 0 $time

